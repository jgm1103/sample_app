<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= stylesheet_link_tag 'blueprint/screen', :method => 'screen' %>
    <%= stylesheet_link_tag 'blueprint/print', :method => 'print' %>
    <%= render 'layouts/stylesheets' %>
  </head>
  <body>
  <div class="container">
    <%= render 'layouts/header' %>
    <section class="round">
      <%= yield %>
    </section>
    <%= render 'layouts/footer' %>
  </body>
</html>
